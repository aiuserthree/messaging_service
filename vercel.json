{
  "version": 2,
  "builds": [
    {
      "src": "admin/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/admin/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/admin/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/admin/$1"
    }
  ]
}
